    "css-loader": "^3.5.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.4",
    "sass": "^1.26.3",
